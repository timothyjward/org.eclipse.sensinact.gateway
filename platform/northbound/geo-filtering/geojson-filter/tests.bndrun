-tester: biz.aQute.tester.junit-platform

-runsystempackages: com.sun.net.httpserver

-runee: JavaSE-1.8
-runvm: ${def;argLine}

-runfw: org.apache.felix.framework
-resolve.effective: active

-runrequires: \
	bnd.identity;id=junit-jupiter-engine,\
	bnd.identity;id=junit-platform-launcher,\
	bnd.identity;id='${project.artifactId}-tests'

	
-runvm.test: \
	-Dorg.eclipse.sensinact.gateway.security.jks.filename=${.}/../cert/keystore.jks,\
	-Dorg.eclipse.sensinact.gateway.security.jks.password=sensiNact_team,\
	-Dorg.eclipse.sensinact.gateway.security.signer.password=sensiNact_team
-runproperties: \
	org.eclipse.sensinact.security.oauth2.config=${.}/src/test/resources/sensinact-security-oauth2.config,\
	org.eclipse.sensinact.gateway.security.jks.filename=${.}/../cert/keystore.jks,\
    org.eclipse.sensinact.gateway.security.jks.password=sensiNact_team,\
    "org.eclipse.sensinact.http.corsheader", false
    org.eclipse.sensinact.gateway.location.latitude=45.2d,\
    org.eclipse.sensinact.gateway.location.longitude=5.7d,\
    org.osgi.service.http.port=8899,\
    org.apache.felix.http.jettyEnabled=true,\
    org.apache.felix.http.whiteboardEnabled=true
-runbundles: \
	ch.qos.logback.core;version='[1.2.3,1.2.4)',\
	com.sun.mail.javax.mail;version='[1.6.2,1.6.3)',\
	geojson-filter;version='[2.0.0,2.0.1)',\
	geojson-filter-tests;version='[2.0.0,2.0.1)',\
	http;version='[2.0.0,2.0.1)',\
	json;version='[20190722.0.0,20190722.0.1)',\
	junit-jupiter-api;version='[5.7.2,5.7.3)',\
	junit-jupiter-engine;version='[5.7.2,5.7.3)',\
	junit-jupiter-params;version='[5.7.2,5.7.3)',\
	junit-platform-commons;version='[1.7.2,1.7.3)',\
	junit-platform-engine;version='[1.7.2,1.7.3)',\
	junit-platform-launcher;version='[1.7.2,1.7.3)',\
	log4j;version='[1.2.17,1.2.18)',\
	org.apache.felix.http.jetty;version='[4.1.10,4.1.11)',\
	org.apache.felix.http.servlet-api;version='[1.1.4,1.1.5)',\
	org.apache.felix.scr;version='[2.1.16,2.1.17)',\
	org.eclipse.jetty.client;version='[9.4.42,9.4.43)',\
	org.eclipse.jetty.util;version='[9.4.42,9.4.43)',\
	org.eclipse.jetty.websocket.client;version='[9.4.42,9.4.43)',\
	org.opentest4j;version='[1.2.0,1.2.1)',\
	org.osgi.service.cm;version='[1.6.0,1.6.1)',\
	org.osgi.service.component;version='[1.4.0,1.4.1)',\
	org.osgi.service.log;version='[1.4.0,1.4.1)',\
	org.osgi.test.common;version='[1.0.1,1.0.2)',\
	org.osgi.test.junit5;version='[1.0.1,1.0.2)',\
	org.osgi.util.function;version='[1.1.0,1.1.1)',\
	org.osgi.util.promise;version='[1.1.1,1.1.2)',\
	org.osgi.util.pushstream;version='[1.0.1,1.0.2)',\
	sensinact-common;version='[2.0.0,2.0.1)',\
	sensinact-core;version='[2.0.0,2.0.1)',\
	sensinact-datastore-api;version='[2.0.0,2.0.1)',\
	sensinact-security-none;version='[2.0.0,2.0.1)',\
	sensinact-signature-validator-api;version='[2.0.0,2.0.1)',\
	sensinact-signature-validator-secure;version='[2.0.0,2.0.1)',\
	sensinact-utils;version='[2.0.0,2.0.1)',\
	slf4j.api;version='[1.7.30,1.7.31)',\
	slf4j.log4j12;version='[1.7.25,1.7.26)'